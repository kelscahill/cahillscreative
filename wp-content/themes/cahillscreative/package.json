{
  "name": "cahills-creative",
  "private": true,
  "browserslist": [
    "extends @roots/browserslist-config"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "setup": "composer install; yarn; yarn build;",
    "start": "bud dev --clean",
    "build": "bud build --clean",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint resources/scripts",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@roots/bud": "6.20.0",
    "@roots/bud-eslint": "6.20.0",
    "@roots/bud-prettier": "6.20.0",
    "@roots/bud-sass": "6.20.0",
    "@roots/bud-stylelint": "6.20.0",
    "@roots/eslint-config": "6.20.0",
    "@roots/sage": "6.20.0"
  },
  "dependencies": {
    "fast-glob": "^3.2.12",
    "in-view": "^0.6.1",
    "jquery": "^3.3.1",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-scss": "^5.0.0"
  }
}
